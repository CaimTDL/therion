{
  "extends": ["next/core-web-vitals"],
  "parserOptions": {
    "ecmaVersion": 2023,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "settings": {
    "react": {
      "version": "latest"
    }
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/jsx-uses-react": "warn",
    "react/jsx-uses-vars": "warn"
  }
}
